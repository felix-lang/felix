        { /* beginning of action switch */
                        case 0: /* must back up */
                        /* undo the effects of YY_DO_BEFORE_ACTION */
                        *yy_cp = yy_hold_char;
                        yy_cp = yy_last_accepting_cpos;
                        yy_current_state = yy_last_accepting_state;
                        goto yy_find_action;

case 1:
YY_RULE_SETUP
{
  newLine();
}
        YY_BREAK
case 2:
YY_RULE_SETUP
{
  UPD_COL;
}
        YY_BREAK
/* -------- comments -------- */
case 3:
YY_RULE_SETUP
{
  /* C-style comments */
  TOKEN_START;
  UPD_COL;
  prevState = YY_START;
  BEGIN(C_COMMENT);
}
        YY_BREAK

case 4:
YY_RULE_SETUP
{
    /* end of comment */
    UPD_COL;
    BEGIN(prevState);
  }
        YY_BREAK
case 5:
YY_RULE_SETUP
{
    /* anything but slash-star or newline -- eat it */
    UPD_COL;
  }
        YY_BREAK
case 6:
YY_RULE_SETUP
{
    newLine();
  }
        YY_BREAK
case YY_STATE_EOF(C_COMMENT):
{
