case 27:
YY_RULE_SETUP
{
    UPD_COL;
    embedded->handle(yytext, yyleng, embedFinish);
  }
        YY_BREAK
case 28:
YY_RULE_SETUP
{
    newLine();
    embedded->handle(yytext, yyleng, embedFinish);
