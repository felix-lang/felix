gen silly () = { for k in (1,2,3) do yield k; done return 99; } 
var ff = silly;
println$ #ff; println$ #ff; println$ #ff; println$ #ff; println$ #ff;

gen stream () = {
  for k in (1,2,3) do yield Some$ k+40; done return None[int];
}

var ll = list_comprehension stream;
println ll;

